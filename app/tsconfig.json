{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@components/*": [
        "./components/*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@helpers/*": [
        "./helpers/*"
      ],
      "@providers/*": [
        "./providers/*"
      ],
      "@routerPaths/*": [
        "./routerPaths/*"
      ]
    }
  },

  "include": [
  "**/*.ts",
  "**/*.tsx",
  ".expo/types/**/*.ts",
  "expo-env.d.ts",
  "app/_layout.tsx",
  "app/(tabs)/(home)/details.tsx",
  "app/old/signin.tsx",
  "app/(tabs)/settings.tsx",
  "app/(tabs)/old"
]
}